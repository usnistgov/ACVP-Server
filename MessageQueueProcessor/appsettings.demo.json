{
  "Serilog": {
    "MinimumLevel": "Warning",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\ACVP.NET_Logs\\MessageQueueProcessor.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:o} [{Level:u3}] ({Application}/{MachineName}/{ThreadId}) {Message}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "MessageQueueProcessor"
    }
  },
  "MessageQueueProcessor": {
    "AutoApprove": {
      "CreateDependency": true,
      "UpdateDependency": true,
      "DeleteDependency": true,
      "CreateImplementation": true,
      "UpdateImplementation": true,
      "DeleteImplementation": true,
      "CreateOE": true,
      "UpdateOE": true,
      "DeleteOE": true,
      "CreatePerson": true,
      "UpdatePerson": true,
      "DeletePerson": true,
      "CreateVendor": true,
      "UpdateVendor": true,
      "DeleteVendor": true,
      "CertifyTestSession": true
    }
  },
  "AllowedHosts": "*"
}
  