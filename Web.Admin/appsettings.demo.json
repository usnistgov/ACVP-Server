{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\ACVP.NET_Logs\\WebAdmin.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:o} [{Level:u3}] ({Application} -> {SourceContext:l}) {Message}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "WebAdmin"
    }
  },
  "LCAVP": {
    "ExtractedFilesRoot": "C:\\LCAVP\\ExtractedFiles",
    "ProcessedFilesRoot": "C:\\LCAVP\\processedFiles",
    "UploadPath": "C:\\LCAVP\\uploads"
  },
  "SsoConfig" : {
    "UseSso": false,
    "AdfsMetadata": "https://sts.nist.gov/FederationMetadata/2007-06/FederationMetadata.xml",
    "WtRealm": "https://admin.demo.acvts.nist.gov",
    "WReply": "https://admin.demo.acvts.nist.gov/"
  },
  "AllowedHosts": "*"
}
  