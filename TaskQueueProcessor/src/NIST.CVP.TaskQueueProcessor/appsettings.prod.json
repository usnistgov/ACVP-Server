{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\ACVP.NET_Logs\\TaskQueueProcessor.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:o} [{Level:u3}] ({Application}/{VsID}/{SourceContext}) {Message}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": ["FromLogContext"],
    "Properties": {
      "Application": "TaskQueueProcessor"
    }
  },
  "TaskQueueProcessorConfig": {
    "PollDelay": 5,
    "MaxConcurrency": 6
  },
  "AllowedHosts": "*"
}
  