{
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\ACVP.NET_Logs\\DataMaintainer.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:o} [{Level:u3}] ({Application}/{MachineName}/{ThreadId}) {Message}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "DataMaintainer"
    }
  },
  "AllowedHosts": "*",
  "DataMaintainer": {
    "TestSessionExpirationEnabled": false,
    "TestSessionExpirationAgeInDays": 30,
    "TestSessionExpirationWarningEmailsEnabled": false,
    "TestSessionDaysBeforeExpirationToWarn": 7,
    "VectorSetCleanupEnabled": true,
    "VectorSetArchiveEnabled": true,
    "VectorSetArchiveFolder": "\\\\172.19.42.193\\ACVPVectorSetArchive"
  }
}
  